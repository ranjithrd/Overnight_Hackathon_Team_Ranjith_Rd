    "definitions": {
        "handlers.AddDepositRequest": {
            "type": "object",
            "required": [
                "amount",
                "user_id"
            ],
            "properties": {
                "amount": {
                    "type": "integer",
                    "example": 10000
                },
                "reference": {
                    "type": "string",
                    "example": "BANK-TX-12345"
                },
                "user_id": {
                    "type": "integer",
                    "example": 1
                }
            }
        },
        "handlers.AddDepositResponse": {
            "type": "object",
            "properties": {
                "ok": {
                    "type": "boolean",
